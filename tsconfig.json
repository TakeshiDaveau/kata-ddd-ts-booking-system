{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "outDir": "build",
    "paths": {
      "@lib/*": [
        "src/lib/*"
      ],
      "@domain/*": [
        "src/domain/*"
      ],
      "@application/*": [
        "src/application/*"
      ],
    }
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}
